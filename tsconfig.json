{
 
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react",
    "lib": [
      "dom",
      "ESNext",
      "DOM.Iterable",
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
  },
  "include": [
    "src"
  ],

}
